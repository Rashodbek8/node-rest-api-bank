generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int       @id @default(autoincrement())
  name     String
  email    String    @unique
  password String
  accounts Account[]
}

model Account {
  id         Int    @id @default(autoincrement())
  userId     Int
  accountNum String @unique
  balance    Float
  user       User   @relation(fields: [userId], references: [id])
}
